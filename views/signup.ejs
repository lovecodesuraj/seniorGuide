<%- include("../partials/header") -%>
  <div class="container-sm signupForm">
    <form action="/signupCongo" method="post">
   
          <input type="text" class="form-control" required name="name" placeholder="Enter your full name">
 
          <input type="email" class="form-control" required name="email" placeholder="Email">
   
          <input type="text" class="form-control" required name="userName" placeholder="User name">
       
            <input type="text" class="form-control" required name="collegeName" placeholder="College/institution Name">
   
            <input type="text" class="form-control" required name="courseName" placeholder="Course Name">
 
            <input type="text" class="form-control" required name="graduationYear" placeholder="Year of graduation">

            <input type="text" class="form-control" required name="branchName" placeholder="Branch Name">
       
          <input type="password" class="form-control" required name="password" placeholder="Password">

          <input type="password" class="form-control" required name="passwordConfirm" placeholder="Confirm Password">
       
      <button type="submit" class="btn btn-primary">Sign in</button>
    </form>
  </div>

  <%- include("../partials/footer") -%>